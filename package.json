{
  "name": "auroratide.com",
  "version": "0.0.0",
  "description": "Timothy Foster's humble website",
  "main": "index.js",
  "scripts": {
    "start": "http-server -p 3000",
    "start:dev": "npm run build && npm-run-all --parallel build:js start",
    "build": "npm-run-all build:js build:html",
    "build:js": "webpack",
    "build:js:watch": "webpack --watch",
    "build:html": "mkdir -p public && cp src/client/index.html public/index.html",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch"
  },
  "author": "Timothy Foster",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "http-server": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
