{
  "name": "auroratide.com",
  "version": "3.0.0",
  "description": "Timothy Foster's humble website",
  "type": "module",
  "scripts": {
    "start": "vite preview",
    "ci": "npm-run-all validate test build",
    "dev": "npm run build:content && npm-run-all --parallel build:components dev:kit",
    "dev:kit": "vite dev",
    "build": "npm-run-all build:components build:content build:kit",
    "build:kit": "vite build",
    "build:content": "node content/_build/index.js",
    "build:components": "rollup -c",
    "clean": "rm -rf static/build static/api build .svelte-kit",
    "test": "jest test",
    "test:smoke": "node smoke/smoke.spec.js",
    "validate": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "components:create": "node scripts/components/create",
    "posts:create": "npm run _posts:create",
    "_posts:create": "node scripts/posts/create",
    "portfolio:create": "node scripts/portfolio/create",
    "art:create": "node scripts/art/create",
    "netlify:update": "node scripts/netlify/update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Auroratide/auroratide.com.git"
  },
  "author": "Timothy Foster",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Auroratide/auroratide.com/issues"
  },
  "homepage": "https://github.com/Auroratide/auroratide.com#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.15.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^4.0.1",
    "@types/jest": "^27.4.0",
    "@types/marked": "^4.0.1",
    "babel-jest": "^27.4.4",
    "commander": "^7.0.0",
    "jest": "^27.4.7",
    "jest-canvas-mock": "^2.4.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^5.0.3",
    "ts-jest": "^27.1.3",
    "tslib": "^2.4.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "@auroratide/img-popout": "^1.0.1",
    "@auroratide/nimcard": "^0.1.2",
    "@auroratide/pokemon-types": "^0.1.0",
    "@auroratide/svelte-custom-element-transitions": "^0.1.1",
    "@auroratide/wait": "^0.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "marked": "^4.0.10",
    "prismjs": "^1.27.0",
    "sudoku": "^0.0.3"
  }
}
